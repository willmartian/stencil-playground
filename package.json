    "prismjs": "^1.24.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/puppeteer": "^5.4.3",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "puppeteer": "^5.5.0"
  },
  "license": "MIT"
}
